{
  "name": "brink-sass",
  "version": "0.1.17",
  "description": "sass functional implementation by brink technology co.",
  "main": "./index.sass",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "kss": "npm run kss:watch",
    "kss:build": "npm run kss:css && npm run kss:coffee && npm run kss:styleguide",
    "kss:styleguide": "kss-node --js ../jquery.min.js --js ../bundle.js --css ../index.css -t ./kss/styleguide-template --destination kss/styleguide --source src --source kss",
    "kss:css": "sass kss/index.sass kss/index.css",
    "kss:coffee": "coffee -c --join kss/bundle.js src",
    "kss:watch": "watch 'npm run kss:build' src tests",
    "server": "node kss/devServer.js"
  },
  "repository": {
    "type": "git",
    "url": "github.com/findthebrink/brink-sass.git"
  },
  "keywords": [
    "sass",
    "css",
    "modular"
  ],
  "author": {
    "name": "Derek Moore",
    "url": "repos@findthebrink.com"
  },
  "license": "MIT",
  "dependencies": {
    "jquery": "^2.1.4",
    "normalize.scss": "^0.1.0"
  },
  "devDependencies": {
    "browserify": "^11.0.0",
    "connect": "^3.4.0",
    "node-minify": "^1.1.0",
    "serve-static": "^1.10.0",
    "watch": "^0.16.0"
  },
