{
  "name": "grocerylist",
  "version": "1.0.0",
  "description": "grocery listing app for cordova",
  "main": "www/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cd www && browser-sync start --server --files '**/*.html, **/*.js, **/*.css'",
    "sass": "sass -wc dev/sass/index.sass:www/css/index.css",
    "coffee": "coffee -wm -o www/js/ dev/coffee/",
    "templates": "watch 'npm run build:templates' dev/coffee/app/templates",
    "build:templates": "jade-amd -f dev/coffee/app/templates -t www/js/app/templates/"
  },
  "author": "derek moore",
  "license": "MIT",
  "devDependencies": {
    "brink-sass": "^0.1.17",
    "browser-sync": "^2.8.2"
  },
  "dependencies": {}
}
